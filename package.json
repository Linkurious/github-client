    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-mocha": "^8.0.0",
    "mocha": "^8.1.1",
    "mocha-junit-reporter": "^2.0.0",
    "mocha-multi-reporters": "^1.1.7",
    "nock": "^13.0.4",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5"
